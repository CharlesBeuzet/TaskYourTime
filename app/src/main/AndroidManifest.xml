<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.taskyourtime">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        >
        <activity android:name=".calendar.AddToCalendarActivity" />
        <activity
            android:name=".todolist.AddItemActivity"
            android:label="@string/title_activity_add_item"
            android:theme="@style/Theme.TaskYourTime.NoActionBar"/>
        <activity android:name=".note.AddNoteActivity" />
        <activity android:name=".DefaultActivity" />
        <activity android:name=".SubscriptionActivity" />
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>